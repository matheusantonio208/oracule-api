{
  "name": "oracule-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/matheusantonio208/oracule-api",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/config/server/server-connect.js",
    "test": "jest --detectOpenHandles",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/config/server/server-connect.js",
    "dev:debug": "ts-node-dev --inspect -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/config/server/server-connect.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jest": "^27.5.1",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^26.5.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "factory-girl": "^5.0.4",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^26.5.2",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "supertest": "^5.0.0",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "bitgener": "^1.2.10",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "ean-check": "^0.1.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-serve-static-core": "^0.1.1",
    "mongoose": "^5.10.9",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.30.6",
    "youch": "^2.0.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty || true"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write",
      "git add ."
    ]
  }
}
